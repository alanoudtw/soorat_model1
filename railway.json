{
    "$schema": "https://railway.app/railway.schema.json",
    
    "build": {
      "builder": "NIXPACKS",
      "nixpacksPlan": {
        "phases": {
            "addtionalProperties": {
                "aptPkgs": ["wget", "tar"]
            }
        }
      }
    },
    "deploy": {
      "startCommand": "fastapi run main.py --port \"$PORT\""
    }
}